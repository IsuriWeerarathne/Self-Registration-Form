			.parent{
				margin:0;
				padding:0;
				background-color:#6c5282;
			}
			.top-main-wrap{
				width:100%;
				height:10%;
				
			}
			.column-left-right {
					float: left;
					width: 20%;
					height:800px;
			}
			.column-middle {
					float: left;
					width: 60%;
					background-color:#c2d3ed;
					height:auto;
					border:#800303;
					border-width:6px;
					border-style:solid;
			}
			
			.row:after {
				content: "";
				display: table;
				clear: both;
			}
			.bottom-part{
				width:100%;
				height:auto;
				
			}
			
		
			
			label{
				font-size:18px;
			}
			
			button {
				padding: 10px;
				margin-top: 10px;
				width: 30%;
				color: white;
				background-color: rgb(41, 57, 194);
				border: none;
				border-radius: 4px;
				align:center;			
			}
			.input-control {
				display: flex;
				flex-direction: column;
				padding-left:15px;
			}
			.input-control input{
				border: 2px solid #f0f0f0;
				border-radius: 4px;
				display: block;
				font-size: 12px;
				padding: 10px;
				width: 80%;
			}
			h3{
				border: 2px solid #f0f0f0;
				border-radius: 4px;
				display: block;
				font-size: 12px;
				padding: 20px;
				width: 75%;
				background-color:white;
				
			}
			h2{
				
				display: block;
				font-size: 18px;
				padding: 20px;
				width: 75%;
				background-color:none;
				
			}
			.input-control input:focus {
				outline: 0;
			}

			.input-control.success input {
				border-color: #09c372;
			}

			.input-control.error input {
				border-color: #ff3860;
			}

			.input-control .error {
				color: #ff3860;
				font-size: 10px;
				height: 13px;
			}